{
    "robotcode.robot.variables": {
        "CMD_VAR": "1",
        "CMD_VAR_LONG": "a test"
    },
    "robotcode.robot.env": {
        "ENV_VAR": "1"
    },
    //"robotcode.debug.attachPython": true,
    "robotcode.debug.defaultConfiguration": {
        "pythonConfiguration": {
            "justMyCode": false
        }
    },
    "robotcode.languageServer.extraArgs": [
        "--verbose",
        "--debugpy",
        //"--debugpy-wait-for-client",
        "--log",
        "--log-level", "DEBUG",
        // "--log-format", "%(indent)s%(levelname)s:%(name)s:%(filename)s:%(lineno)d: %(message)s"
    ],
    // "robotcode.robocop.configFile": "pyproject.toml",
    "robotcode.robocop.enabled": true,
    "robotcode.robocop.exclude": [
        "E0303"
    ],
    "robotcode.analysis.referencesCodeLens": false,
    "robotcode.analysis.diagnosticMode": "openFilesOnly",
    "robotcode.analysis.progressMode": "detailed",
    "robotcode.analysis.findUnusedReferences": true,
    "robotcode.analysis.cache.saveLocation": "workspaceFolder",
    "robotcode.analysis.cache.ignoredLibraries": [
        //"robot.libraries.Remote",
        "**/lib/alibrary.py",
        "LibraryWithErrors"
    ],
    "robotcode.robot.variableFiles": [
        "${EXECDIR}/resources/testvars.yml"
    ],
    "[robotframework]": {
        "editor.wordBasedSuggestions": "off",
        "editor.semanticHighlighting.enabled": true,
        "editor.inlayHints.enabled": "offUnlessPressed",
        "editor.quickSuggestions": {
            "strings": "on"
        }
    },
    "robotcode.debug.attachPython": true,
    "debugpy.debugJustMyCode": false,

}