    "version": "0.2.0",
    "configurations": [
        {
            "name": "RobotCode: Run Current",
            "type": "robotcode",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "target": "${file}"
        },
        {
            "name": "RobotCode: Run All",
            "type": "robotcode",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "target": "."
        },
        {
            "name": "RobotCode: Default",
            "type": "robotcode",
            "request": "launch",
            "purpose": "default",
            "presentation": {
                "hidden": true
            },
            "attachPython": false,
            "pythonConfiguration": "RobotCode: Python"
        },
        {
            "name": "RobotCode: Default",
            "type": "robotcode",
            "request": "launch",
            "purpose": "test",
            "presentation": {
                "hidden": true
            },
            "attachPython": true,
            "pythonConfiguration": "RobotCode: Python",
            "robotCodeArgs": [
                "-v",
                // "--log",
                // "--log-level=TRACE",
                // "--log-calls"
            ],
            "profiles": [
                "ci*",
                "firefox",
                "headless"
            ],
            // "args": [
            //     "--loglevel",
            //     "TRACE:TRACE",
            // ],
            "internalConsoleOptions": "neverOpen"
        },
        {
            "name": "RobotCode: Python",
            "type": "python",
            "request": "attach",
            "presentation": {
                "hidden": true
            },
            "justMyCode": false
        }
    ]
}